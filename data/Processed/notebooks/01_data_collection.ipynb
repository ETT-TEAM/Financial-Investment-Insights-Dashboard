import yfinance as yf
import pandas as pd

stocks = ["AAPL", "MSFT", "GOOGL"]

data = {}
for stock in stocks:
    ticker = yf.Ticker(stock)
    df = ticker.history(period="2y")
    df.reset_index(inplace=True)
    data[stock] = df

# Save each stock
for stock, df in data.items():
    df.to_csv(f"../data/raw/{stock}_raw.csv", index=False)
